@{
    ViewData["Title"] = "AccessDenied";
}
@using Microsoft.AspNetCore.Authentication

<h1>@ViewData["Title"]</h1>

<p>Sorry Access Denied.</p>

@if (User.Identity.IsAuthenticated)
{
    <h2>Claims</h2>

    <dl>
        @foreach (var claim in User.Claims)
        {
            <dt>@claim.Type</dt>
            <dd>@claim.Value</dd>
        }
    </dl>
}